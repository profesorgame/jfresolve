<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>Jfresolve</AssemblyName>
    <RootNamespace>Jfresolve</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Jellyfin.Model" Version="10.11" />
    <PackageReference Include="Jellyfin.Controller" Version="10.11" />
    <PackageReference Include="Jellyfin.Common" Version="10.11" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Config\Config.html" />
    <None Remove="Config\config.html" />
    <None Update="manifest.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="jfresolve.png">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <EmbeddedResource Include="Config\config.html" />
    <EmbeddedResource Include="jfresolve.png" />
  </ItemGroup>

</Project>
